{
    "author": "Giovanni León Corujo",
    "name": "vita-weight",
    "description": "Aplicación de seguimiento de peso",
    "version": "0.6.0",
    "homepage": "/tabs/tab1",
    "scripts": {
        "android": "ionic build --prod && npx cap sync && ionic cap copy && ionic cap open android",
        "dev": "ionic serve --port 8080",
        "dev:test": "ionic serve --port 8080 --configuration=test",
        "test:all": "npm run test && npm run test:e2e:run && npm run allure:generate",
        "test": "jest",
        "test:e2e": "cypress open  --e2e --browser chrome",
        "test:e2e:run": "npx cypress run --browser chrome --env allure=true",
        "allure:open": "allure open allure-results",
        "allure:generate": "allure generate allure-results --clean -o allure-report && allure open",
        "doc": "npx compodoc --tsconfig tsconfig.json --disableSourceCode -d documentation --serve",
        "sonar": "node scripts/run-sonar.js",
        "lint": "ng lint",
        "format": "prettier --write \"src/**/*.{ts,html,css,scss,json}\""
    },
    "private": true,
